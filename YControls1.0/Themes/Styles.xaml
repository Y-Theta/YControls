                                        Width="{Binding ViewPortSize.Width, RelativeSource={RelativeSource Mode=TemplatedParent}}"
                                        Background="{x:Null}">
                                    <Path x:Name="BackPanel"
                                          StrokeThickness="{TemplateBinding DotSize}"
                                          Stroke="{TemplateBinding Background}" />
                                    <Path x:Name="ValuePanel"
                                          Stroke="{TemplateBinding Foreground}" />
                                </Canvas>
                            </Grid>
