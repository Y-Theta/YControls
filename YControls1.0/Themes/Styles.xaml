<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:slide="clr-namespace:YControls.SlideControls"
                    xmlns:button="clr-namespace:YControls.FontIconButtons"
                    xmlns:winx="clr-namespace:YControls.WinXStyleControls"
                    xmlns:flow="clr-namespace:YControls.FlowControls"
                    xmlns:conv="clr-namespace:YControls.Converters">

    <FontFamily x:Key="Segoe">
        pack://application:,,,/YControls;component/Resources/Segoe.ttf#Segoe
    </FontFamily>

    <x:Static x:Key="VisibilityConverter"
              Member="conv:VisibilityConverter.Singleton" />
    <x:Static x:Key="StringConverter"
              Member="conv:StringConverter.Singleton" />
    <x:Static x:Key="NumberConverter"
              Member="conv:NumberConverter.Singleton" />
    <x:Static x:Key="ColorConverter"
              Member="conv:ColorConverter.Singleton" />

    <!--#region                                                         Frame-->
    <Style x:Key="DefaultFrame"
           TargetType="Frame">
        <Setter Property="Margin"
                Value="0" />
        <Setter Property="Padding"
                Value="0" />
        <Setter Property="Background"
                Value="Black" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Frame">
                    <Border BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            Padding="{TemplateBinding Padding}">
                        <ContentPresenter x:Name="PART_FrameCP" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--#endregion-->

    <!--#region                                                         ButtonAbout-->

    <!--#endregion-->

    <!--#region                                                         ContextMenu-->

    <!--WinXTaskBarContextMenuStyle-->
    <Style x:Key="WinXTaskBarContextMenuStyle"
           TargetType="flow:YT_ContextMenu">
        <Setter Property="Padding"
                Value="2,5" />
        <Setter Property="Width"
                Value="180" />
        <Setter Property="BorderThickness"
                Value="1" />
        <Setter Property="Placement"
                Value="MousePoint" />
        <Setter Property="BorderBrush"
                Value="#A0A0A0" />
        <Setter Property="Background"
                Value="#2B2B2B" />
    </Style>

    <!--WinXTaskBarContextMenuItemStyle-->
    <Style x:Key="WinXTaskBarContextMenuItemStyle"
           TargetType="flow:YT_MenuItem">
        <Setter Property="Width"
                Value="NaN" />
        <Setter Property="IsCheckable"
                Value="False" />
        <Setter Property="FontFamily"
                Value="{StaticResource Segoe}" />
        <Setter Property="HorizontalAlignment"
                Value="Stretch" />
        <Setter Property="VerticalAlignment"
                Value="Stretch" />
        <Setter Property="HorizontalContentAlignment"
                Value="Stretch" />
        <Setter Property="VerticalContentAlignment"
                Value="Stretch" />
        <Setter Property="LabelFontSize"
                Value="12" />
        <Setter Property="FontSize"
                Value="14" />
        <Setter Property="Height"
                Value="32" />
        <Setter Property="Margin"
                Value="0,1" />
        <Setter Property="IconWidth"
                Value="24" />
        <Setter Property="LabelTextAlignment"
                Value="Left" />
        <Setter Property="LabelHorizontalAlignment"
                Value="Stretch" />
        <Setter Property="LabelFontWeight"
                Value="Light" />
        <Setter Property="IconMargin"
                Value="8,0" />
        <Setter Property="LabelFgNormal"
                Value="White" />
        <Setter Property="LabelFgOver"
                Value="White" />
        <Setter Property="LabelFgPressed"
                Value="White" />
        <Setter Property="LabelBgNormal"
                Value="Transparent" />
        <Setter Property="LabelBgOver"
                Value="#22FFFFFF" />
        <Setter Property="LabelBgPressed"
                Value="#22FFFFFF" />
        <Setter Property="IconFgNormal"
                Value="White" />
        <Setter Property="IconFgOver"
                Value="White" />
        <Setter Property="IconFgPressed"
                Value="White" />
        <Setter Property="IconBgNormal"
                Value="Transparent" />
        <Setter Property="IconBgOver"
                Value="#22FFFFFF" />
        <Setter Property="IconBgPressed"
                Value="#22FFFFFF" />
    </Style>

