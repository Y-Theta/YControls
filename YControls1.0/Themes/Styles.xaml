                                                                       Storyboard.TargetProperty="(UIElement.RenderTransform).(TranslateTransform.X)">
                                            <EasingDoubleKeyFrame KeyTime="0"
                                                                  Value="0" />
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.167"
                                                                  Value="0" />
                                            <SplineDoubleKeyFrame x:Name="E2Frame1"
                                                                  KeySpline="0.4,0,0.6,1"
                                                                  KeyTime="0:0:1.167" />
                                            <EasingDoubleKeyFrame x:Name="E2Frame2"
                                                                  KeyTime="0:0:2.167" />
                                            <SplineDoubleKeyFrame x:Name="E2Frame3"
                                                                  KeySpline="0.4,0,0.6,1"
                                                                  KeyTime="0:0:3.167" />
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="E3"
                                                                       Storyboard.TargetProperty="(UIElement.RenderTransform).(TranslateTransform.X)">
                                            <EasingDoubleKeyFrame KeyTime="0"
                                                                  Value="0" />
                                            <EasingDoubleKeyFrame KeyTime="0:0:0.333"
                                                                  Value="0" />
                                            <SplineDoubleKeyFrame x:Name="E3Frame1"
